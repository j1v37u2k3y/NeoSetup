{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://localhost"
    },
    {
      "pattern": "^http://127.0.0.1"
    },
    {
      "pattern": "^https://127.0.0.1"
    }
  ],
  "httpHeaders": [
    {
      "urls": [
        "https://github.com/"
      ],
      "headers": {
        "User-Agent": "Mozilla/5.0 (compatible; NeoSetup-LinkChecker/1.0)"
      }
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 2,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [
    200,
    206,
    302,
    304
  ]
}
